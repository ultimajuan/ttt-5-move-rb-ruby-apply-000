#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go?"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
user_input = gets.strip
new_input = input_to_index(user_input)
move(board, new_input)
display_board(board)